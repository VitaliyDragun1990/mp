<?xml version="1.0" encoding="UTF-8"?>

<entity-mappings xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence/orm
                                http://xmlns.jcp.org/xml/ns/persistence/orm_2_1.xsd"
                 version="2.1">
                 
    <mapped-superclass class="org.myphotos.domain.entity.AbstractEntity">
    	<entity-listeners>
    		<entity-listener class="org.myphotos.repository.jpa.listener.CreatedNowListener"></entity-listener>
    	</entity-listeners>
    </mapped-superclass>

	<entity class="org.myphotos.security.AccessToken">
		<entity-listeners>
			<entity-listener class="org.myphotos.repository.jpa.listener.AccessTokenLifeCycleManager">
			</entity-listener>
		</entity-listeners>
	</entity>
                 
</entity-mappings>